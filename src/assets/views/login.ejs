    <body class="container columns"
        style="width: 100vw;height: 100vh;; display: flex; justify-content: center; align-items: center;">
        <div data-aos="zoom-in" class="card card-shadow col-8" style="background-color: #fce69a;">

            <div class="card-body">

                <div class="columns">
                    <div class="column">
                        <img src="/img/login.gif" class="col-12">
                    </div>
                    <div class="column col-4 p-2">
                        <h3 class="h3 text-center text-dark text-bold">Login</h3>
                        <div class="form-group" id="error-input">
                            <label class="form-label text-tiny text-center w100">Use your agentbanking ID &
                                password</label>
                            <input onfocus="removeError()" class="form-input input-lg has-error" type="text" id="user"
                                placeholder="User ID">
                            <input onfocus="removeError()" class="form-input my-2 input-lg " type="password" id="passwd"
                                placeholder="Password">
                            <p id="error" style="opacity:0;" class="form-input-hint">Invalid Login Information</p>
                            <p id="errorInfo" class="form-input-hint"></p>
                            <button id="submit" id="error-btn" class="btn btn-primary w100"
                                onclick="authorized()">Login</button>
                            <button id="submit" id="error-btn my-2" class="btn btn-link text-dark disabled w100"
                                onclick="authorized()">Forget
                                Password</button>
                        </div>
                        <!-- <button id="submit" id="error-btn my-2" class="btn btn-secondary w100" onclick="guestlogin()">Login
                        as Anonymous</button> -->
                    </div>
                </div>
                <!-- <img src="/img/Witctcraft-1x1bright.svg" style="height: 1.5rem;"> -->
                <div style="display:flex; flex-direction: column; justify-content: center; align-items: center;">

                    <img src="/img/autometa/SVG/aflogo-solid-bright-h.svg" style="height: 2rem;">

